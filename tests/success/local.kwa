class paire {
  int x;
  int y;

  void constructor(int x, int y) {
    this.x = x;
    this.y = y;
  }

  int somme() {
    return this.x + this.y;
  }
}

class triple extends paire {
  int z;

  void constructor(int x, int y, int z) {
    int c;
    
    print(k);
