point p;

class point {
  int x;
  int y;

  void constructor(int a, int b) {
    this.x = a;
    this.y = b;
  }

  int somme() {
    return this.x + this.y;
  }

  int somme(int x, int y) {
    return x + y;
  }
}

main {
  p = new point(2, 1);
  p.x = 3 + p.x;
  print(48 + p.somme());
  print(p.somme(40, 2));
  // affiche 54 puis 42
}
